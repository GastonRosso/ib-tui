{
  "name": "ib-tui",
  "version": "0.1.0",
  "description": "Terminal UI for Interactive Brokers",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "ib-tui": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "lint": "eslint src/ scripts/",
    "lint:fix": "eslint src/ scripts/ --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc -p tsconfig.typecheck.json --noEmit",
    "gateway:start": "tsx scripts/start-gateway.ts",
    "gateway:stop": "docker compose down"
  },
  "keywords": [
    "ibkr",
    "interactive-brokers",
    "tui",
    "trading",
    "terminal"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/react": "^19.2.10",
    "@types/readline-sync": "^1.4.8",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "globals": "^17.3.0",
    "ink-testing-library": "^4.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "dependencies": {
    "@stoqey/ib": "^1.3.0",
"ink": "^4.4.1",
    "react": "^18.2.0",
    "readline-sync": "^1.4.10",
    "zustand": "^4.5.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
